@model IEnumerable<WalletWebUtility.Models.MobileLogin>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.walletno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.username)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.walletno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.username)
            </td>
            <td>
                @Html.ActionLink("Details ", "Details", new { id = item.walletno }, new { @class = "btn btn-info" })

            </td>
        </tr>
    }
</table>
